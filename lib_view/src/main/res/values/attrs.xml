<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PixelSpinner">
        <attr name="pop_direction" format="enum">
            <enum name="up" value="0"/>
            <enum name="down" value="1"/>
            <enum name="left" value="2"/>
            <enum name="right" value="3"/>
        </attr>

        <attr name="pop_width" format="dimension"/>
        <attr name="pop_height" format="dimension"/>
    </declare-styleable>
</resources>